<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXDrawersStack?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?scenebuilder-stylesheet ../styles/style.css?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.school.main.controllers.ClassroomController">
    <children>
        <StackPane prefHeight="150.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                  <SplitPane fx:id="centerSplitPane" dividerPositions="1.0" focusTraversable="true" prefHeight="160.0" prefWidth="200.0">
                      <items>
                          <AnchorPane fx:id="viewStack" prefHeight="200.0" prefWidth="200.0">
                              <children>
                        <JFXDrawersStack fx:id="viewDraw" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                  <ToggleButton fx:id="leftToggleButton" mnemonicParsing="false" selected="true" styleClass="navigation-button" text="" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" />
                              </children>
                          </AnchorPane>
                          <AnchorPane fx:id="chatStack" minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0" styleClass="content-pane">
                     <children>
                        <JFXDrawersStack fx:id="chatDraw" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="0.0" />
                     </children>
                  </AnchorPane>
                      </items>
                  </SplitPane>
            </children>
        </StackPane>
    </children>
</AnchorPane>
