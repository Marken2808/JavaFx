            <TreeTableColumn fx:id="agilityCol" minWidth="-1.0" prefWidth="100.0" text="Agility" />
            <TreeTableColumn fx:id="defendingCol" minWidth="-1.0" prefWidth="100.0" text="Defending" />
            <TreeTableColumn fx:id="physicalCol" minWidth="-1.0" prefWidth="100.0" text="Physical" />
        </columns>
      </TreeTableView>
   </children>
</AnchorPane>
